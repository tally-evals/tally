{
  "name": "@tally-evals/examples-ai-sdk",
  "version": "0.0.0",
  "private": true,
  "description": "AI SDK LLM evaluation examples using the Tally framework",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "tui": "pnpm -C ../../.. --filter @tally-evals/cli build && node ../../../packages/cli/dist/bin.mjs browse",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "ai": "^5.0.89",
    "@ai-sdk/google": "^2.0.29",
    "zod": "^3.23.8",
    "@tally-evals/core": "workspace:*",
    "@tally-evals/tally": "workspace:*",
    "@tally-evals/trajectories": "workspace:*"
  },
  "devDependencies": {
    "@tally-evals/typescript-config": "workspace:*",
    "@tally-evals/biome-config": "workspace:*",
    "@types/node": "^24.6.2",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "vitest": "^4.0.16",
    "dotenv": "^17.2.3"
  }
}


