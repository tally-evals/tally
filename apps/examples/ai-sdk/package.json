{
  "name": "@tally-evals/examples-ai-sdk",
  "version": "0.0.0",
  "private": true,
  "description": "AI SDK LLM evaluation examples using the Tally framework",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "cli:build": "bun run --filter @tally-evals/cli build",
    "tui": "bun run cli:build && bunx tally browse",
    "dev:server": "bun run cli:build && bunx tally dev server",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "ai": "^5.0.121",
    "@ai-sdk/google": "^2.0.52",
    "@tally-evals/cli": "workspace:*",
    "zod": "^3.23.8",
    "@tally-evals/core": "workspace:*",
    "@tally-evals/tally": "workspace:*",
    "@tally-evals/trajectories": "workspace:*"
  },
  "devDependencies": {
    "@tally-evals/typescript-config": "workspace:*",
    "@tally-evals/biome-config": "workspace:*",
    "@types/bun": "latest",
    "@types/node": "^24.6.2",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "dotenv": "^17.2.3"
  }
}


