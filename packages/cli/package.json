{
  "name": "@tally-evals/cli",
  "version": "0.0.0",
  "description": "A CLI for the Tally evaluation framework",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "tally": "./dist/bin.mjs"
  },
  "typesVersions": {
    "*": {
      "*": ["./dist/index.d.ts"]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tally-evals/tally.git",
    "directory": "packages/cli"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "clean": "rm -rf dist"
  },
  "keywords": ["tally", "evaluation", "llm", "ai", "cli"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.4",
    "commander": "^12.1.0",
    "ink": "^5.0.1",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1",
    "@tally-evals/core": "workspace:*",
    "@tally-evals/tally": "workspace:*"
  },
  "peerDependencies": {
    "@tally-evals/tally": "workspace:*"
  },
  "devDependencies": {
    "@tally-evals/biome-config": "workspace:*",
    "@tally-evals/typescript-config": "workspace:*",
    "@types/node": "24.6.2",
    "@types/react": "^18.3.3",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.4",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "ink": "^5.0.1",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  }
}
